{
    "name": "ruvents/doctrine-bundle",
    "description": "RUVENTS Doctrine Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov@ruvents.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "doctrine/dbal": "^2.5",
        "doctrine/orm": "^2.5",
        "symfony/config": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/http-kernel": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Ruvents\\DoctrineBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
